    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/petewarden/projects/tflite_face/build/bin/tflite_face",
            "args": [],
            "stopAtEntry": false,
            "cwd": "/home/petewarden/projects/tflite_face/",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "/home/petewarden/projects/tflite_face/lib"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}